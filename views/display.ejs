<%- include parcial/headder.ejs -%>
    

    
    <table class="table">
        <thead>
          <tr>
            <th scope="col">Movie Name</th>
            <th scope="col">Director</th>
            <th scope="col">Actor</th>
            <th scope="col">Actress</th>
            <th scope="col">Action</th>
          </tr>
        </thead>
        <% Movie.forEach(function(post){%>
       
          <tr>
            <th scope="row"><%= post.movie %></th>
            <td><%= post.director %></td>
            <td><%= post.actor %></td>
            <td><%= post.actress %></td>
            <td> <a href="/delete/<%= post._id%>" class="btn btn-danger">DELETE</a> </tr>
          <% }) %>
      </table> 
   

